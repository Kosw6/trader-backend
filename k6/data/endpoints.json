              { "target": 120, "duration": "90s" },
              { "target": 160, "duration": "90s" },
              { "target": 200, "duration": "90s" },
              { "target": 200, "duration": "180s" }
            ],
            "thresholds": {
              "http_req_failed": ["rate<0.05"],
              "http_req_duration": ["p(95)<3000"]
            }
          }
        ]
      },
